<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
	http://www.springframework.org/schema/beans/spring-beans-2.5.xsd">

    <bean id="genericDAOimpl" class="com.cuellojuan.dao.impl.GenericDAOImpl">
        <property name="dataSource" ref="dataSource" />
    </bean>


    <bean id="customerDAO" class="com.cuellojuan.dao.CustomerDAO">
        <property name="dataSource" ref="dataSource" />
    </bean>

    <bean id="alumnoDAO" class="com.cuellojuan.dao.AlumnoDAO">
        <property name="dataSource" ref="dataSource" />
    </bean>

    <bean id="customer" class="com.cuellojuan.entity.Customer">
    </bean>

    <bean id="alumno" class="com.cuellojuan.entity.Alumno">
    </bean>


</beans>